{
    "manifest_version": 3,
    "name": "Sustainability Score Extension",
    "version": "1.0",
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://www.amazon.com/*"],  
        "js": ["content.js"]
      }
    ]
  }